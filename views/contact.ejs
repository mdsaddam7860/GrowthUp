<%- include("header") -%>

<!-- Contact section start Here -->

<section id="contactUsgrowthup" action="contactdbs" method="post">
  <div class="contactSection">
    <div class="content">
      <div class="left-side">
        <div class="address details">
          <i class="fas fa-map-marker-alt"></i>
          <div class="topic">Address</div>
          <div class="text-one">Near Lnjpt Chapra</div>
          <div class="text-two">Chapra, Saran, Bihar</div>
        </div>
        <div class="phone details">
          <i class="fas fa-phone-alt"></i>
          <div class="topic">Phone</div>
          <div class="text-one">+91 8969910869</div>
        </div>
        <div class="email details">
          <i class="fas fa-envelope"></i>
          <div class="topic">Email</div>
          <div class="text-one">growthupconsultancy@gmail.com</div>
          <div class="text-two">artistaryanchauhan@gmail.com</div>
        </div>
      </div>
      <div class="right-side">
        <div class="topic-text">Unlocking Your Startup's Potential</div>
        <p>
          Have a question, need advice, or want to explore how our consulting
          services can benefit your startup? We'd love to hear from you! Please
          use the form below to send us a message, and our team will get back to
          you promptly.
        </p>
        <form action="contactdbs" method="post">
          <div class="input-box">
            <input type="text" name="name" placeholder="Enter your name" />
          </div>
          <div class="input-box">
            <input type="tel" name="phone" placeholder="Mobile Number" />
          </div>
          <div class="input-box">
            <input type="text" name="email" placeholder="Enter your email" />
          </div>
          <div class="input-box message-box">
            <textarea name="message" placeholder="Enter your message"></textarea>
          </div>
          <div class="button">
            <button>
            <input type="button" value="Send Now" />
          </button>
          </div>
        </form>
      </div>
    </div>
  </div>
</section>

<!-- Contact section End Here -->

<%- include("footer") -%>
